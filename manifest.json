{
  "name": "ebk Reader",
  "short_name": "ebk",
  "id": "ebk-reader",
  "display": "standalone",
  "scope": "/ebk-reader/",
  "start_url": "/ebk-reader/",
  "theme_color": "#f7f7f7",
  "background_color": "#edeef0",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "/ebk-reader/#book",
      "accept": {
        "application/*": [".epub", ".mobi"]
      }
    }
  ]
}
